            function encode(element, key, list){
                var list = list || [];

                if (typeof element === 'object') {
                    for (var idx in element) {
                        encode(element[idx], key ? key + '[' + idx + ']' : idx, list);
                    }
                } else {
                    list.push(key+'='+encodeURIComponent(element));
                }

                return list.join('&');
